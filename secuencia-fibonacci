import unittest

def fibonacci(n):
    if n <= 0:
        raise ValueError("La posiciÃ³n tiene que ser un numero entero positivo")

    if n == 1:
        return 0

    if n == 2:
        return 1

    a, b = 0, 1
    for _ in range(3, n + 1):
        a, b = b, a + b

    return b

class Test(unittest.TestCase):

    def test_fibonacci_position_9(self):
        expected_result = 21
        result = fibonacci(9)
        self.assertEqual(result, expected_result)


if __name__ == "__main__":
    unittest.main()
